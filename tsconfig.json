{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "traceResolution": false
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "include": [
    "src/**/*",
    "*config.ts"
  ]
}