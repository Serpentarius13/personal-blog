---
import { ProseConfig } from "@/components/react/prose-config/ProseConfig";
import Layout from "@/layouts/base";
import { getCollection } from "astro:content";

if (import.meta.env.PROD) return Astro.redirect("/");

const postName = "astro-blog-from-scratch";
const post = await getCollection("posts", ({ slug }) => slug === postName).then((r) => r[0]);
---

<Layout noIndex title="" description="">
  <ProseConfig client:only="preact" />
</Layout>
